<% if controller_name != 'sessions' %>
  <%= link_to "ログインページへ戻る", new_session_path(resource_name), class: 'Button' %>
  <br/>
<% end %>
<% if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規登録", new_registration_path(resource_name), class: 'Button' %>
  <br/>
<% end %>